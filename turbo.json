{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "check-types": {
            "dependsOn": ["build"]
        },
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": ["^@elizaos/core#build"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
